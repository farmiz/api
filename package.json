  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --timeout 20000 --recursive build/tests/src/**  --exit",
    "ci-test": "cross-env NODE_ENV=test nyc mocha --timeout 20000 --recursive build/tests/src/**  --exit",
    "start-build": "npm run clean-build && tsc -v && tsc -p tsconfig.json -w",
    "build": "npm run clean-build && tsc -v && tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=development nodemon --config nodemon.json --verbose --signal SIGINT --watch ./build --exec \"node build/index.js\"",
    "createModel": "node ./build/commands/createModel.js",
    "clean-build": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage ./build",
    "type": "module",
    "build:docs": "apidoc -i ./controllers -o ./docs/",
    "prod-build": "tsc -p tsconfig.json",
    "prod-start": "cross-env NODE_ENV=production pm2 ./build/index.js && pm2 monit",
    "staging-ci": "npm run prod-build && npm run ci-test && npm run prod-start"
  },
